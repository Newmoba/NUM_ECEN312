NET "clk_50mhz" LOC = "C9" | IOSTANDARD = LVCMOS33 | PERIOD = 20 ns;

NET "sw<0>" LOC = "F12" | IOSTANDARD = LVCMOS33;  -- Check your board manual
NET "sw<1>" LOC = "E12" | IOSTANDARD = LVCMOS33;
NET "sw<2>" LOC = "D13" | IOSTANDARD = LVCMOS33;
NET "sw<3>" LOC = "C13" | IOSTANDARD = LVCMOS33;

NET "btn_south" LOC = "K17" | IOSTANDARD = LVCMOS33 | PULLUP;
NET "btn_east"  LOC = "H18" | IOSTANDARD = LVCMOS33 | PULLUP;

# 8 Discrete LEDs (LD7 left-most - LD0 right-most)
NET "ld<0>" LOC = "H12" | IOSTANDARD = LVCMOS33 | DRIVE = 8 | SLEW = SLOW;
NET "ld<1>" LOC = "J11" | IOSTANDARD = LVCMOS33 | DRIVE = 8 | SLEW = SLOW;
NET "ld<2>" LOC = "K11" | IOSTANDARD = LVCMOS33 | DRIVE = 8 | SLEW = SLOW;
NET "ld<3>" LOC = "G12" | IOSTANDARD = LVCMOS33 | DRIVE = 8 | SLEW = SLOW;
NET "ld<4>" LOC = "G10" | IOSTANDARD = LVCMOS33 | DRIVE = 8 | SLEW = SLOW;
NET "ld<5>" LOC = "H10" | IOSTANDARD = LVCMOS33 | DRIVE = 8 | SLEW = SLOW;
NET "ld<6>" LOC = "J10" | IOSTANDARD = LVCMOS33 | DRIVE = 8 | SLEW = SLOW;
NET "ld<7>" LOC = "K13" | IOSTANDARD = LVCMOS33 | DRIVE = 8 | SLEW = SLOW;

# DDR SDRAM pins (MIG wizard автоматаар өгнө, эсвэл UG230 Appendix B-ээс ав)
